
<table class="table table-bordered">
  <tr>
    <th>Name</th>
    <th>Email</th>
  </tr>
  <% @users.each do | user |%>
  <tr>
    <td><%= user.name %></td>
    <td><%= user.email%></td>
    <td><%= link_to "Edit", edit_user_path(user) %></td>
    <td><%= link_to "Delete", user, method: :delete, data: {confirm: "Are you sure!"}%></td>
  </tr>
  <%end%>
</table>

<%= link_to "+User", new_user_path%>
